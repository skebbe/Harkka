<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="fi.harkka.model.Application" table="Application">
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="fname" column="fname" type="string"/>
      <property name="lname" column="lname" type="string"/>
       <property name="dob" column="dob" type="date"/>
      <property name="gender" column="gender" type="string"/>
      <property name="application" column="application" type="string"/>
   </class>
</hibernate-mapping>